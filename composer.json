{
    "name": "IamKelvin/sitemaplrv",
    "description": "Create and generate sitemaps with ease",
    "keywords": [
        "IamKelvin",
        "sitemaplrv"
    ],
    "homepage": "https://github.com/IamKelvin/sitemaplrv",
    "license": "MIT",
    "authors": [
        {
            "name": "The Streak",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6",
        "illuminate/support": "~5.3.0",
        "nesbot/carbon": "^1.21",
        "spatie/crawler": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "orchestra/testbench": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "IamKelvin\\Sitemap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IamKelvin\\Sitemap\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
  }
}
